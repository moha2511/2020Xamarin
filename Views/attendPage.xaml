<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="adamBozhiProj.attendPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Skirv din mail her for at tilmelde dig!" />
            <Entry x:Name ="emailEntry" Placeholder="E-Mail"></Entry>
            <Button x:Name="attend" Clicked="Attend_Clicked" Text="Tilmeld"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>